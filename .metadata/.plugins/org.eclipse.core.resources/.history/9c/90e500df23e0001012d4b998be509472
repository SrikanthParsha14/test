#include <stdio.h>
#include <stdlib.h>
#include <asm-generic/errno.h>

void print_message()
{
    printf("Hi, \n");
    puts("!!!Hello World!!!"); /* prints !!!Hello World!!! */
}

int main(void) {
	print_message();
	return EXIT_SUCCESS;
}
