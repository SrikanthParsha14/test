#include <stdio.h>
#include <stdlib.h>
#include <asm-generic/errno.h>

int main(void) {
	printf("Hi, \n");
	puts("!!!Hello World!!!"); /* prints !!!Hello World!!! */
	return EXIT_SUCCESS;
}
